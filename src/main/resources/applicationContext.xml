<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">



     <bean name="add" class="com.model.Address">
     <property name="street" value="1109"/>
     <property name="city" value="noida"/>
     <property name="state" value="Uttar Pradesh"/>
     </bean>

    <bean name="emp" class="com.model.Employee">
    <property name="id" value="1"/>
     <property name="salary" value="10000000"/>
      <property name="name" value="Gaurav"/>
       <property name="gender" value="male"/>
       <property name="address" ref="add"/>
    
    
    </bean>
